{
	"manifest_version" : 3,
	"name": "Checklist RGESN",
	"description": "Aide à l'auto-évaluation de la conformité d’un service numérique selon le Référentiel Général d'Écoconception de Services Numériques (RGESN)",
	"version": "1.0.0",
	"icons": {
		"16": "./static/ecodesign-addon-logo-16x16.png",
		"48": "./static/ecodesign-addon-logo-48x48.png",
		"128": "./static/ecodesign-addon-logo-128x128.png"
	},
	"action": {
		"default_icon": "./static/ecodesign-addon-logo-48x48.png",
		"default_popup": "./index.html",
		"default_title": "Checklist RGESN"
	},	
	"permissions": [
		"storage"
	]
}